//Patient Bills:
SELECT FIRST_NAME, LAST_NAME, PATIENT_INFO.PATIENT_NO, BILL_DETAILS.ROOM_NO, BI_NO, TOTAL_CHARGES
FROM PATIENT_INFO
INNER JOIN BILL_DETAILS
ON PATIENT_INFO.PATIENT_NO = BILL_DETAILS.PATIENT_NO
ORDER BY LAST_NAME;

//Patient’s selected Doctors:
SELECT PATIENT_INFO.FIRST_NAME AS 'PATIENT.FNAME', PATIENT_INFO.LAST_NAME AS 'PATIENT.LNAME', PATIENT_INFO.ROOM_NO, PATIENT_NO, DOCTOR.FIRST_NAME AS 'DOCTOR.FNAME', DOCTOR.LAST_NAME AS 'DOCTOR.LNAME', NPI
FROM  PATIENT_INFO
INNER JOIN DOCTOR
ON PATIENT_INFO.ROOM_NO = DOCTOR.ROOM_NO
ORDER BY ROOM_NO, PATIENT_INFO.LAST_NAME, DOCTOR.LAST_NAME; 
 
