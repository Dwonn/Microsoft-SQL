# Hospital Management Database
//A. PATIENT_INFO:
CREATE TABLE PATIENT_INFO(
PATIENT_NO Int NOT NULL, 
FIRST_NAME VARCHAR(30) NOT NULL,
LAST_NAME VARCHAR(30) NOT NULL,
AGE INT NOT NULL,
GENDER VARCHAR(30) NOT NULL,
ADDRESS VARCHAR(90) NOT NULL,
DATE DATE NOT NULL,
CONTACT_NUMBER VARCHAR(90),
ROOM_NO VARCHAR(50)
FOREIGN KEY(ROOM_NO) REFERENCES ROOM,
PRIMARY KEY(PATIENT_NO));


//B. BILL_DETAILS:
CREATE TABLE BILL_DETAILS(
BI_NO Int NOT NULL,
PATIENT_NO INT NOT NULL,
ROOM_NO VARCHAR(50) NOT NULL,
TOTAL_CHARGES VARCHAR(100),
FOREIGN KEY(PATIENT_NO) REFERENCES PATIENT_INFO,
FOREIGN KEY(ROOM_NO) REFERENCES ROOM,
PRIMARY KEY(BI_NO));

//C.ROOM:
CREATE TABLE ROOM( 
ROOM_NO VARCHAR(30) NOT NULL,
ROOM_TYPE  VARCHAR(50) 
PRIMARY KEY(ROOM_NO));

//D.DOCTOR:
CREATE TABLE DOCTOR(
NPI INT NOT NULL,
FIRST_NAME VARCHAR(20) ,
LAST_NAME VARCHAR(30) NOT NULL,
ADDRESS VARCHAR(150),
PHONE_NUMBER VARCHAR(90),
QUALIFICATION VARCHAR(100),
GENDER VARCHAR(30) NOT NULL,
ROOM_NO VARCHAR(30),
PRIMARY KEY(NPI));

